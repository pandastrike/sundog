{
  "name": "sundog",
  "version": "0.0.2",
  "description": "Wrapper for the JavaScript AWS SDK, powered by Fairmont to give it a functional boost",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "node_modules/.bin/coffee --compile --transpile --inline-map --output lib/ src/",
    "postpublish": "(json -f package.json version | xargs -I version git tag -am version version) && git push --tags",
    "test": "coffee --transpile --inline-map test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pandastrike/sundog.git"
  },
  "keywords": [
    "AWS",
    "functional",
    "cloud",
    "serverless"
  ],
  "contributors": [
    {
      "name": "Dan Yoder",
      "email": "dan@pandastrike.com"
    },
    {
      "name": "David Harper",
      "email": "david@pandastrike.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pandastrike/sundog/issues"
  },
  "homepage": "https://github.com/pandastrike/sundog#readme",
  "dependencies": {
    "fairmont": "2.0.1",
    "mime": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "coffeescript": "2.1.0",
    "json": "^9.0.6",
    "source-map-support": "^0.5.0"
  }
}
